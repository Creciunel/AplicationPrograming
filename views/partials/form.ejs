<form method="post" class="container w-75 mt-2">
    <p class="fs-1 text-center">Registru</p>
    <input
        name="name"
        class="form-control form-control-lg"
        type="text"
        placeholder="Nume"
        aria-label=".form-control-lg example"
    />
    <input
        name="lastaName"
        class="form-control"
        type="text"
        placeholder="Prenume"
        aria-label="default input example"
    />
    <input
        name="nota"
        class="form-control form-control-sm"
        type="text"
        placeholder="Nota"
        aria-label=".form-control-sm example"
    />
    <div class="d-grid gap-2 col-6 mx-auto mt-5">
        <input
            type="submit"
            value="Success"
            class="btn btn-outline-success"
        />
    </div>
</form>